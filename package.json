{
  "name": "angular-stock-charts",
  "version": "1.0.0",
  "description": "View charts of companies' stock price histories",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --inline --hot --content-base ./public",
    "test": "mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "lint": "eslint .",
    "start:prod": "node server.js",
    "build:prod": "webpack --optimize-minimize",
    "postinstall": "npm run build:prod",
    "simulate-prod": "set NODE_ENV=production&&npm run build:prod&&npm run start:prod"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "engines": {
    "node": "4.4.5",
    "npm": "3.10.5"
  },
  "dependencies": {
    "angular": "^1.5.7",
    "angular-animate": "^1.5.7",
    "angular-toastr": "^1.7.0",
    "babel": "^6.5.2",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.8.0",
    "bootstrap": "^3.3.6",
    "css-loader": "^0.23.1",
    "express": "^4.13.4",
    "file-loader": "^0.9.0",
    "highcharts": "^4.2.5",
    "highcharts-ng": "0.0.11",
    "html-loader": "^0.4.3",
    "node-sass": "^3.8.0",
    "normalize.css": "^4.2.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0"
  },
  "devDependencies": {
    "eslint": "^2.10.2",
    "webpack": "^1.13.0"
  }
}
